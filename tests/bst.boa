(fun (emptybst)
    (vek false false false)
)

(fun (insertbst root value)
    (if (isbool root) 
        (vek value false false)
        (if (isbool (index root 0))
            (vek value false false)
            (if (= value (index root 0))
                root
                (if (< value (index root 0))
                    (vek (index root 0) (insertbst (index root 1) value) (index root 2))
                    (vek (index root 0) (index root 1) (insertbst (index root 2) value))
                )
            )
        )
    )
)

(fun (searchbst root value)
    (if (isbool root)
        false
        (if (isbool (index root 0))
            false
            (if (= value (index root 0))
                true
                (if (< value (index root 0))
                    (searchbst (index root 1) value)
                    (searchbst (index root 2) value)
                )
            )
        )
    )
)

